/* pages/morelist/morelist.wxss */
.music_tj_list {
  width: 48%;
  float: left;
  margin-right: 15rpx;
}
.music_img image {
  width: 100%;
  height: 280rpx;
  /* border-radius: 150rpx; */
}
.music_js text {
  font-size: 32rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}